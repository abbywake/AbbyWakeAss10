@{
    ViewData["Title"] = "Home Page";
}

@model IndexViewModel
@using AbbyWakeAss10.Infrastructure 


@* making the team name appear at the top  *@
<h1>@Model.TeamCategory</h1>
<br />
@* Printing out the info for each bowler  *@
@foreach (var x in Model.Bowler)
{
<b><p>@x.BowlerFirstName @x.BowlerMiddleInit @x.BowlerLastName</p></b>
    <p>@x.BowlerAddress , @x.BowlerCity @x.BowlerState @x.BowlerZip</p>
    <p>@x.BowlerPhoneNumber</p>
   
}

@*//bottom page tag with made css info*@
<div class="btn btn btn-link">
    <div page-info="@Model.PageNumberInfo" page-action="Index" page-classes-enabled="true" page-class="btn"
         page-class-normal="button-outline-dark"
         page-class-selected="btn-primary"
         page-url-teamName="@Model.TeamCategory"
         class="btn-group pull-right m-1"></div>
</div>